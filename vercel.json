{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "buildCommand": "npm run vercel-build",
                "outputDirectory": "dist/demo/browser",
                "framework": "angular"
            }
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            },
            "dest": "/assets/$1"
        },
        {
            "src": "/manifest.webmanifest",
            "headers": {
                "cache-control": "public, max-age=0, must-revalidate"
            },
            "dest": "/manifest.webmanifest"
        },
        {
            "src": "/ngsw-worker.js",
            "headers": {
                "cache-control": "public, max-age=0, must-revalidate"
            },
            "dest": "/ngsw-worker.js"
        },
        {
            "src": "^/[^.]+$",
            "headers": {
                "cache-control": "public, max-age=0, must-revalidate"
            },
            "dest": "/index.html"
        },
        {
            "handle": "filesystem"
        }
    ]
}