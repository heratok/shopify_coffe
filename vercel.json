{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "buildCommand": "npm run build",
                "outputDirectory": "dist/demo/browser"
            }
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/manifest.webmanifest",
            "dest": "/manifest.webmanifest"
        },
        {
            "src": "/ngsw-worker.js",
            "dest": "/ngsw-worker.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}